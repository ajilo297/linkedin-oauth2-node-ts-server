{
  "name": "linkedin-oauth-server",
  "version": "0.0.1",
  "description": "Test server to test linkedin OAuth 2.0 integration",
  "main": "./dist/server.js",
  "scripts": {
    "test": "jest --forceExit --runInBand --detectOpenHandles",
    "test-coverage": "jest --forceExit --runInBand --coverage --detectOpenHandles",
    "build": "echo \"Compiling JS from TypeScript. Compiled Artifacts will be add to .\\dist \" && tsc",
    "prod": "npm run build && npm run start",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "watch-node": "nodemon ./dist/src/server.js",
    "watch-ts": "tsc -w",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "start": "npm run serve",
    "serve": "node ./dist/src/server.js"
  },
  "keywords": [
    "LinkedIn",
    "OAuth 2.0"
  ],
  "author": "Ajil Oommen <ajil@altorumleren.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "project-name": "^1.0.0",
    "prom-client": "^11.3.0",
    "response-time": "^2.3.2",
    "stringify-object": "^3.3.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/stringify-object": "^3.2.0",
    "@types/uuid": "^3.4.4",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/expect": "^1.20.4",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.121",
    "@types/project-name": "^1.0.0",
    "@types/response-time": "^2.3.3",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "jest": "^24.1.0",
    "jest-html-reporter": "^2.5.0",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.3.3333",
    "yargs": "^13.2.1"
  }
}
